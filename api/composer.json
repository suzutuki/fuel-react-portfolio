{
    "name": "fuel/fuel",
    "type": "project",
    "description": "FuelPHP Framework Application",
    "keywords": ["framework", "php", "fuelphp"],
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "composer/installers": "~1.0",
        "fuel/core": "1.8.*",
        "fuel/auth": "1.8.*",
        "fuel/email": "1.8.*",
        "fuel/oil": "1.8.*",
        "fuel/orm": "1.8.*",
        "fuel/parser": "1.8.*"
    },
    "extra": {
        "installer-paths": {
            "fuel/packages/{$name}/": ["type:fuel-package"]
        }
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
